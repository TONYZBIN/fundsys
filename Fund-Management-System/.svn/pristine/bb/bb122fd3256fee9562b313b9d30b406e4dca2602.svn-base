package edu.fjut.fundsys.domain;

public class Feature extends ValueObject {
	private Integer featureId=1;
	private String featureTitle;
	private String featureDescribe;
	private String featureInfo;

	public Integer getFeatureId() {
		return featureId;
	}

	public void setFeatureId(Integer featureId) {
		this.featureId = featureId;
	}

	public String getFeatureTitle() {
		return featureTitle;
	}

	public void setFeatureTitle(String featureTitle) {
		this.featureTitle = featureTitle;
	}

	public String getFeatureDescribe() {
		return featureDescribe;
	}

	public void setFeatureDescribe(String featureDescribe) {
		this.featureDescribe = featureDescribe;
	}

	public String getFeatureInfo() {
		return featureInfo;
	}

	public void setFeatureInfo(String featureInfo) {
		this.featureInfo = featureInfo;
	}

	public Feature(String featureTitle, String featureDescribe,
			String featureInfo) {
		super();
		this.featureTitle = featureTitle;
		this.featureDescribe = featureDescribe;
		this.featureInfo = featureInfo;
	}
    
}
