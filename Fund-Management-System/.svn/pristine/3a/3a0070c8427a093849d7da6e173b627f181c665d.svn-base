package edu.fjut.fundsys.helper;

public class ClientInfosQueryHelper {
	private String qryClientId;
	private String qryClientName;
	private String qryClientSex;
	private String qryClientActive;

	public String getQryClientId() {
		return qryClientId;
	}

	public void setQryClientId(String qryClientId) {
		this.qryClientId = qryClientId;
	}

	public String getQryClientName() {
		return qryClientName;
	}

	public void setQryClientName(String qryClientName) {
		this.qryClientName = qryClientName;
	}

	public String getQryClientSex() {
		return qryClientSex;
	}

	public void setQryClientSex(String qryClientSex) {
		this.qryClientSex = qryClientSex;
	}

	public String getQryClientActive() {
		return qryClientActive;
	}

	public void setQryClientActive(String qryClientActive) {
		this.qryClientActive = qryClientActive;
	}

	public ClientInfosQueryHelper(String qryClientId, String qryClientName,
			String qryClientSex, String qryClientActive) {
		super();
		this.qryClientId = qryClientId;
		this.qryClientName = qryClientName;
		this.qryClientSex = qryClientSex;
		this.qryClientActive = qryClientActive;
	}

	@Override
	public String toString() {
		return "ClientInfosQueryHelper [qryClientId=" + qryClientId
				+ ", qryClientName=" + qryClientName + ", qryClientSex="
				+ qryClientSex + ", qryClientActive=" + qryClientActive + "]";
	}

}
