package edu.fjut.fundsys.domain;

public class FundType extends ValueObject {
	private Integer fundTypeId = 1000;
	private String fundTypeName;
	private String fundTypeDescribe;

	public String getFundTypeDescribe() {
		return fundTypeDescribe;
	}

	public void setFundTypeDescribe(String fundTypeDescribe) {
		this.fundTypeDescribe = fundTypeDescribe;
	}

	public Integer getFundTypeId() {
		return fundTypeId;
	}

	public void setFundTypeId(Integer fundTypeId) {
		this.fundTypeId = fundTypeId;
	}

	public String getFundTypeName() {
		return fundTypeName;
	}

	public void setFundTypeName(String fundTypeName) {
		this.fundTypeName = fundTypeName;
	}

	public FundType(String fundTypeName) {
		super();
		this.fundTypeName = fundTypeName;
	}

	public FundType() {
		super();
	}

	public FundType(String fundTypeName, String fundTypeDescribe) {
		super();
		this.fundTypeName = fundTypeName;
		this.fundTypeDescribe = fundTypeDescribe;
	}

	@Override
	public String toString() {
		return "FundType [fundTypeId=" + fundTypeId + ", fundTypeName="
				+ fundTypeName + "]";
	}

}
